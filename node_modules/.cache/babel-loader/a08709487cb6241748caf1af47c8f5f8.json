AA;AAAA,eAAA,OAAA,CAAA,MAAA,CAAA,KAAA,CAAA;AAAA,OALA,EAFA,CAQA;AACA;AArBA,GA3BA;AAkDA,EAAA,OAlDA,qBAkDA;AAAA;;AACA,IAAA,KAAA,CAAA,GAAA,CAAA,qDAAA,EACA,IADA,CACA,UAAA,GAAA;AAAA,aAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA;AAAA,KADA,EAEA,KAFA,CAEA,UAAA,KAAA;AAAA,aAAA,OAAA,CAAA,MAAA,CAAA,KAAA,CAAA;AAAA,KAFA;AAGA;AAtDA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <Header/>\n    <AddTodo v-bind:todos=\"todos\" v-on:add-todo=\"addTodo\"/>\n    <Todos v-bind:todos=\"todos\" v-on:del-todo=\"deleteTodo\"/>\n  </div>\n</template>\n\n<script>\n\n\nimport Todos from '../components/Todos';\nimport AddTodo from '../components/AddTodo';\nimport axios from 'axios';\n\nrequire('@/assets/css/main.css');\n\nexport default {\n  name: 'App',\n  components: {\n    Todos,\n    AddTodo,\n  },\n  data() {\n    return {\n      todos: [\n        // {\n        //   id: 1,\n        //   title: \"Todo One\",\n        //   completed: false\n        // },\n        // {\n        //   id: 2,\n        //   title: \"Todo Two\",\n        //   completed: true\n        // },\n        // {\n        //   id: 3,\n        //   title: \"Todo Three\",\n        //   completed: false\n        // }\n      ]\n    }\n  },\n  methods: {\n    deleteTodo(id)\n    {\n      //this.todos = this.todos.filter(todo => todo.id !== id);\n      axios.delete(`https://jsonplaceholder.typicode.com/todos/${id}`)\n      .then(res => {\n        this.todos = this.todos.filter(todo => todo.id !== id)\n        return res;\n      })\n      .catch(error => Promise.reject(error))\n    }\n    , addTodo(newTodo)\n    {\n      const { title, completed } = newTodo;\n      axios.post('https://jsonplaceholder.typicode.com/todos', {\n        title,\n        completed\n      })\n      .then(res => this.todos = [...this.todos, res.data])\n      .catch(error => Promise.reject(error))\n      //this.todos = [...this.todos, newTodo];\n    }\n  },\n  created() {\n    axios.get('https://jsonplaceholder.typicode.com/todos?_limit=5')\n      .then(res => this.todos =  res.data)\n      .catch( error => Promise.reject(error))\n  }\n}\n\n</script>\n\n<style>\n/* #app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n} */\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\nbody {\n  font-family: Arial, Helvetica, sans-serif;\n}\n</style>\n"],"sourceRoot":"src/views"}]}